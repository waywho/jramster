<!-- Scaffolding generated by Casein v5.1.1.5 -->

<h2>Pages<%= casein_pagination_details @pages %></h2>
<%= form_tag edit_multiple_casein_pages_path, method: :post do %>
	<%= render :partial => 'casein/pages/table', :locals => { :pages => @pages } %>
	<%= render :partial => 'casein/layouts/multi_btns' %>
<% end %>

<div class="text-center">
	<%= will_paginate @pages %>
</div>

<%= content_for :sidebar do %>

	<li><%= link_to "#{casein_show_icon "plus-sign"}Add a new page".html_safe, new_casein_page_path %></li>
	Download Data: <%= link_to 'CSV', casein_pages_path(format: 'csv') %> | 
		<%= link_to 'Excel', casein_pages_path(format: 'xlsx') %>
	<br />
	<br />
	Import Data with CSV file:
	<br />
		<%= form_tag import_casein_pages_path, multipart: true do %>
			<%= file_field_tag :file, accept: 'text/csv' %>
			<%= submit_tag "Import", :class => 'btn btn-xs btn-primary pull-right' %>
		<% end %>


<% end %>

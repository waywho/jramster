<div class = 'col-xs-offset-2 col-xs-8'>
	<table>
		<tr>
			<th>Page Title</th>
			<th>Last Update</th>
			<th>Actions</th>
		</tr>
		<% @pages.each do |page| %>
		<tr>
			<td><%= page.title %></td>
			<td><%= page.updated_at.to_s(:short) %></td>
			<td><%= link_to 'Edit', edit_admin_page_path(page) %></td>
		</tr>
		<% end %>

	</table>
</div>

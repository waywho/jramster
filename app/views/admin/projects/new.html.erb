<div class = 'col-xs-offset-2 col-xs-8'>
	<%= simple_form_for @project, :url => admin_projects_path do |f| %>
	<%= f.input :title %>
	<%= f.input :overview, input_html: {:id => 'eg-basic', :class => 'boo text-small'} %>
	<%= f.input :details, input_html: {:id => 'eg-basic', :class => 'boo text-small'} %>
	<%= f.input :feature_image %>
	<%= f.button :submit %>
	<% end %>
<div>


<script type="text/javascript">
  $(document).ready(function(){

	$(function() {
	    $('.boo').editable({
	      inlineMode: false
	    })
  	});

  })
</script>